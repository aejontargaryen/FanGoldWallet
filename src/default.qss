================================================LINE-TEXT-EDIT BARS=========================================
*/
QLineEdit {
	border: 1px solid #d4bd13;
	border-radius: 8px;
	padding: 2px 8px;
	background: black;
	selection-background-color: gray;
	height: 23px;
}

QTextEdit {
	border: 1px solid #d4bd13;
	border-radius: 8px;
	padding: 2px 8px;
	background: black;
	selection-background-color: #cecece;
}

QLineEdit:focus, QTextEdit:focus {
	border: 2px solid qconicalgradient(cx:0.5, cy:0.5, angle:173.9, stop:0 rgba(136, 106, 22, 255), stop:0.104478 rgba(166, 140, 41, 255), stop:0.169154 rgba(204, 181, 74, 255), stop:0.268657 rgba(235, 219, 102, 255), stop:0.333333 rgba(245, 236, 112, 255), stop:0.422886 rgba(209, 190, 76, 255), stop:0.487562 rgba(187, 156, 51, 255), stop:0.537313 rgba(168, 142, 42, 255), stop:0.606965 rgba(202, 174, 68, 255), stop:0.666667 rgba(218, 202, 86, 255), stop:0.746269 rgba(208, 187, 73, 255), stop:0.800995 rgba(187, 156, 51, 255), stop:1 rgba(137, 108, 26, 255));
	border-radius: 8px;
	background: black;
}

/* ----------------------------end---line text-edit bars-----------------------------------------------------




==============================================SPIN-COMBO-BOXes=================================================
*/
QSpinBox, QDoubleSpinBox, QComboBox {
	border: 2px outset #d0bd4b;
	border-radius: 8px;
	padding: 2px 8px;
	background-color: black;
	height: 25px;
	selection-color: black;
	selection-background-color: #EDD400;
}

QSpinBox::up-button, QDoubleSpinBox::up-button {
	subcontrol-origin: border;
	subcontrol-position: top right;
	border: 1px solid black;
	border-radius: 4px;
	width: 16px;
	outline: none;
	margin-right: 4px;
	margin-top: 4px;
	background-color: black;
}


QSpinBox::down-button, QDoubleSpinBox::down-button {
	subcontrol-origin: border;
	subcontrol-position: bottom right;
	border: 1px solid black;
	border-radius: 4px;
	width: 16px;
	outline: none;
	margin-right: 4px;
	margin-bottom: 4px;
	background-color: black;
}

QSpinBox::up-arrow, QDoubleSpinBox::up-arrow {
	image: url(:/icons/up_arrow.png);
	width: 10px;
	height: 10px;
	color: black;
}

QSpinBox::down-arrow, QDoubleSpinBox::down-arrow, QComboBox::down-arrow {
	image: url(:/icons/down_arrow.png);
	width: 10px;
	height: 10px;
	color: black;
}

QComboBox::down-arrow:on {
	top: 1px;
	left: 1px;
}

QComboBox::drop-down {
	subcontrol-origin: padding;
	subcontrol-position: top right;
	width: 15px;
	border: none;
	selection-color: black;
	selection-background-color: #EDD400;
}

QComboBox QAbstractItemView {
	border: none;
	background: black;
	color: white;
	selection-color: black;
	selection-background-color: #EDD400;
}

QComboBox:item {
	padding: 0 0 0 20px;
	height: 25px;
	margin: 0;
	selection-color: black;
	selection-background-color: #EDD400;
	border: none;
}

QComboBox:item:checked {
	color: white;
	font-family: capsuula;
	font-weight: bold;
	padding: 0 0 0 20px;
	background-color: black;
}
QComboBox:item:selected {
	color: black;
	background-color: #EDD400;
	border: none;
}

QSpinBox:focus, QDoubleSpinBox:focus, QComboBox:focus {
	border: 3px solid #d3bf4f; 
	border-radius: 8px;
	background: black;
}


/*					
--------------------------------------end-spin combo boxes--------------------------------------------



====================================================SLIDER and SCROLL=====================================
*/

QSlider {
	background: transparent;
	background-image: url();
	height: 30px;
	border-radius: 5px;
}

QSlider::groove:horizontal {
	border: 3px ridge #9e9100;
	background: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:0, stop:0 rgba(255, 247, 164, 255), stop:1 rgba(201, 183, 0, 255));
	height: 5px;
	border-radius: 5px;
	
}

QSlider::sub-page:horizontal {
	background: transparent;
	background-image: url();
	border: 2px solid #ffd633;
	height: 5px;
	border-radius: 5px;
}

QSlider::add-page:horizontal {
	background: black;
	border: 2px solid #ffd633;
	height: 5px;
	border-radius: 5px;
}

QSlider::handle:horizontal {
	background: transparent;
	border: none;
	width: 16px;
	height: 16px;
	margin: 0;
	margin-top: -8px;
	margin-bottom: -8px;
	border-radius: 4px;
	outline: none;
	image: url(:/images/handle.png);
}


QCheckBox:unchecked { color: darkred; }
QCheckBox:checked { color: #aeaeae; }



QAbstractScrollArea#m_transfersScrollarea {
	background-color: transparent;
	margin: 0;
	padding: 0;
}

QWidget#scrollAreaWidgetContents {
	background-color: transparent;
}

QToolBar#accountToolBar { 
	background-color: qlineargradient(spread:pad, x1:0.505, y1:0, x2:0.49, y2:0.988636, stop:0 rgba(5, 5, 5, 255), stop:0.115 rgba(15, 15, 15, 255), stop:0.3 rgba(30, 30, 30, 255), stop:0.75 rgba(30, 30, 30, 255), stop:0.885 rgba(15, 15, 15, 255), stop:1 rgba(5, 5, 5, 255));
	border: 0; min-height: 71px;
}


/*===================================================TOOLBAR===============================================
*/
QToolBar { 
	spacing: 0;
	padding: 0;
	margin:	0;
}

QToolBar#toolBar {
	background-color: qlineargradient(spread:reflect, x1:0.495, y1:1, x2:0.52, y2:0, stop:0 rgba(38, 38, 38, 255), stop:0.49 rgba(0, 0, 0, 252), stop:0.984293 rgba(0, 0, 0, 255));
	width: 180px; 
	min-width: 150px; 
	spacing: 0; 
	padding: 0; 
	margin: 0;
	border: 0;
	font-size: 16px;
}

QToolBar#toolBar QToolButton{
	background-color: transparent;
	text-align: center;
	text-transform: uppercase;
	border: 3px double transparent;
	padding: 15px 10px 15px 10px;
	margin-left: 8px;
	spacing:0px;
	width: 180px;
	color: #f7f7f5;
	border-radius: 30px;
	min-width: 150px;
}

